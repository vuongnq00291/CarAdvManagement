# Home page
GET     /                           controllers.Application.index
GET     /CarAdv/:id         controllers.CarAdvController.get(id:Int)
GET     /CarAdvs            controllers.CarAdvController.gets
POST    /CarAdv             controllers.CarAdvController.post
PUT     /CarAdv/:id             controllers.CarAdvController.put(id:Int)
DELETE  /CarAdv/:id             controllers.CarAdvController.delete(id:Int)
# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)

    